{
  "name": "paws",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "lint": "npm run lint:branch-name && npm run lint:types && npm run lint:eslint && npm run lint:prettier",
    "lint:branch-name": "npx validate-branch-name",
    "lint:types": "npx tsc --noEmit",
    "lint:eslint": "npx eslint ./src --ext js,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:prettier": "npx prettier --check ./src/**/*",
    "lint:prettier:fix": "npx prettier --write ./src/**/*",
    "lint:staged": "npx lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "preview": "vite preview",
    "prepare": "test -n \"$DISABLE_POSTINSTALL\" || npx husky",
    "release": "npx semantic-release"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^12.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transformer-svg": "^2.0.2",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.1.1",
    "prettier": "^3.2.5",
    "sass": "^1.74.1",
    "semantic-release": "^23.0.6",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "validate-branch-name": "^1.3.0",
    "vite": "^5.2.0",
    "vite-plugin-checker": "^0.6.4",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "validate-branch-name": {
    "pattern": "^(master|main|develop){1}$|^(feature|fix|bug|release)\/.+$",
    "errorMsg": "Invalid branch name, please read docs/git-conventions.md"
  }
}
